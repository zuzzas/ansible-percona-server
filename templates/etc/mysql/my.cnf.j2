# {{ ansible_managed }}

{% for section, attributes in percona_server_etc_my_cnf.items() %}

[{{ section }}]
{% for name, value in attributes.items() %}
{% if value %}
{{ name }} = {{ value }}
{% else %}
{{ name }}
{% endif %}
{% endfor %}
{% endfor %}

{% if percona_server_etc_my_cnf_includedir is defined %}
!includedir {{ percona_server_etc_my_cnf_includedir }}
{% endif %}
